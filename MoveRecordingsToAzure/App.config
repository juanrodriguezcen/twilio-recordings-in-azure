<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <add key="GetRecordings.DaysOldRequired" value="10"/>
    <add key="GetRecordings.PageSize" value="100"/>

    <!-- Replace this with your twilio credentials -->
    <add key="Twilio.AccountSID" value="XXXXXXXXXXXXXXX"/>
    <add key="Twilio.AuthToken" value="XXXXXXXXXXXXXXX"/>

    <add key="Azure.StorageConnectionString" value="XXXXXXXXXXXXXXX"/>
    <add key="Azure.ContainerName" value="twilio-recordings"/>

    <add key="MaxThreads" value="4"/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>